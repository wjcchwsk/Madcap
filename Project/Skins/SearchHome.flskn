<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="3"
  SkinType="Component"
  Comment="HTML5 Search Bar skin"
  ComponentType="SearchBar"
  EnableResponsiveOutput="True">
  <Stylesheet>
    <Styles>
      <Style
        Name="Search Bar">
        <Properties>
          <Property
            Name="BorderTopLeftRadius">0px</Property>
          <Property
            Name="BorderTopRightRadius">0px</Property>
          <Property
            Name="BorderBottomRightRadius">0px</Property>
          <Property
            Name="BorderBottomLeftRadius">0px</Property>
          <Property
            Name="Height">4em</Property>
          <Property
            Name="BorderTop">solid 1px #d3d3d3</Property>
          <Property
            Name="BorderRight">solid 1px #d3d3d3</Property>
          <Property
            Name="BorderBottom">solid 1px #d3d3d3</Property>
          <Property
            Name="BorderLeft">solid 1px #d3d3d3</Property>
          <Property
            Name="Width">100%</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Submit Button">
        <Properties>
          <Property
            Name="BackgroundColor">transparent</Property>
          <Property
            Name="BackgroundImage">url('icon-search-gray_1.png')</Property>
        </Properties>
      </Style>
    </Styles>
    <Resources>
      <Resource
        Name="icon-search-gray.png"></Resource>
      <Resource
        Name="icon-search-gray_1.png"></Resource>
    </Resources>
  </Stylesheet>
  <Strings>
    <Stylesheet
      lang="en-us">
      <Stylesheet>
        <Styles>
          <Style
            Name="Search Input">
            <Properties>
              <Property
                Name="Placeholder">Search our help site...</Property>
            </Properties>
          </Style>
        </Styles>
      </Stylesheet>
    </Stylesheet>
  </Strings>
</CatapultSkin>